{
  "name": "@apibara/starknet",
  "version": "0.1.1",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "prebuild": "pnpm run protobuf && pnpm run gen-types",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --fix src --ext .ts",
    "protobuf": "mkdir -p dist/proto && cp src/proto/*.proto dist/proto",
    "gen-types": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./src/proto -I src/proto ./src/proto/starknet.proto"
  },
  "dependencies": {
    "google-protobuf": "^3.21.2",
    "long": "^5.2.1",
    "protobufjs": "^7.1.2",
    "ts-proto": "^1.131.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9"
  }
}
