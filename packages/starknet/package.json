{
  "name": "@apibara/starknet",
  "version": "2.0.0",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "exports": "./src/index.ts",
  "publishConfig": {
    "files": ["dist", "src", "README.md"],
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "pnpm build:proto && unbuild",
