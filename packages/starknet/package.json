    "unbuild": "^2.0.0",
    "viem": "^2.13.8",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@apibara/protocol": "workspace:*",
    "@effect/schema": "^0.67.15",
    "effect": "^3.2.6",
    "long": "^5.2.1",
    "nice-grpc-common": "^2.0.2",
    "protobufjs": "^7.1.2"
  }
